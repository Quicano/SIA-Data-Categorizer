	<fontspec id="291" size="20" family="BPBKQG+TimesNewRomanPS" color="#000000"/>
	<fontspec id="292" size="11" family="WQOOYC+TimesNewRomanPSMT" color="#000000"/>
	<fontspec id="293" size="18" family="BPBKQG+TimesNewRomanPS" color="#000000"/>
