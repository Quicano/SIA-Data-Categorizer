	<fontspec id="337" size="20" family="XPOQJK+TimesNewRomanPS" color="#000000"/>
	<fontspec id="338" size="13" family="QRBBFZ+TimesNewRomanPSMT" color="#000000"/>
	<fontspec id="339" size="18" family="XPOQJK+TimesNewRomanPS" color="#000000"/>
	<fontspec id="340" size="11" family="XPOQJK+TimesNewRomanPS" color="#000000"/>
	<fontspec id="341" size="11" family="QRBBFZ+TimesNewRomanPSMT" color="#000000"/>
	<fontspec id="342" size="9" family="QRBBFZ+TimesNewRomanPSMT" color="#000000"/>
