	<fontspec id="559" size="18" family="PVWVRN+TimesNewRomanPSMT" color="#000000"/>
	<fontspec id="560" size="16" family="PVWVRN+TimesNewRomanPSMT" color="#000000"/>
	<fontspec id="561" size="20" family="PVWVRN+TimesNewRomanPSMT" color="#000000"/>
